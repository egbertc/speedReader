<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uchicago.egbertc.profinaladv.RsvpFXMLController">
    <stylesheets>
        <URL value="@/styles/rsvpCSS.css" />
