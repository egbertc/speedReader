                        <TableView fx:id="tblFiles" layoutX="67.0" layoutY="86.0" onDragDropped="#fileDrag" prefHeight="200.0" prefWidth="466.0" AnchorPane.bottomAnchor="85.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="80.0">
