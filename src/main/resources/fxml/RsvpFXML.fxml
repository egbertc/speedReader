                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <Button fx:id="btnFileFinder" layoutX="274.0" layoutY="31.0" mnemonicParsing="false" onAction="#fileFinder" text="Open" />
